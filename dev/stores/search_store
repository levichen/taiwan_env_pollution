var Reflux = require('reflux');
var Actions = require('../actions/Actions');

var SearchStore = Reflux.createStore({
	listenables: Actions,
	init: function() {
		this.enabled = false;
		
	},
	getInitialState: function() {
        return {
            enabled: this.enabled
        };
    },
    onDoSearch: function(search_val) {
    	var data = {
    		enabled: true
    	};
    	this.trigger(data);
    }
});

module.exports = SearchStore;